<div class="edit_profile">
    <div class="edit_profile-photo">
        {{!-- {{> PhotoSVG}} --}}
        <img src="../../assets/svg/photo.svg"/>
    </div>
    <div class="f-title-bold edit_profile-username">{{user.display_name}}</div>
    <form class="form__edit f-normal">
        <div class="form__edit_row">
            <div>Почта</div>{{{editEmail}}}
        </div>
        <div class="form__edit_row">
            <div>Логин</div>{{{editLogin}}}
        </div>
        <div class="form__edit_row">
            <div>Имя</div>{{{editFirstName}}}
        </div>
        <div class="form__edit_row">
            <div>Фамилия</div>{{{editLastName}}}
        </div>
        <div class="form__edit_row">
            <div>Имя в чате</div>{{{editDisplayName}}}
        </div>
        <div class="form__edit_row">
            <div>Телефон</div>{{{editPhone}}}
        </div>
    </form>
    {{{ButtonProfile}}}
</div>
